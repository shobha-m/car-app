{
    "name": "search",
    "version": "1.0.0",
    "description": "Search app",
    "main": "index.js",
    "scripts": {
        "build": "webpack-dev-server --inline --progress --port 8080 --config webpack.config.dev.js",
        "build:prod": "del-cli dist && ngc -p tsconfig.aot.json && ngc -p tsconfig.aot.json && webpack --config webpack.config.prod.js --progress --profile --bail && del-cli 'src/app/**/*.js' 'src/app/**/*.ngfactory.ts' 'src/app/**/*.js.map' 'src/app/**/*.shim.ts' 'src/app/**/*.ngsummary.json' 'src/app/**/*.ngstyle.ts' 'dist/app'",
        "serve": "lite-server",
        "test": "karma start ./karma/karma.conf.js",
        "test:once": "karma start karma.conf.js --single-run"
    },
    "author": "Shobha",
    "license": "ISC",
    "dependencies": {
        "@angular/animations": "^4.3.0",
        "@angular/common": "^4.3.0",
        "@angular/compiler": "^4.3.0",
        "@angular/compiler-cli": "^4.3.0",
        "@angular/core": "^4.3.0",
        "@angular/forms": "^4.3.0",
        "@angular/http": "^4.3.0",
        "@angular/platform-browser": "^4.3.0",
        "@angular/platform-browser-dynamic": "^4.3.0",
        "@angular/platform-server": "^4.3.0",
        "@angular/router": "^4.3.0",
        "@angular/upgrade": "^4.3.0",
        "core-js": "^2.4.1",
        "es6-shim": "^0.35.3",
        "rxjs": "^5.4.2",
        "zone.js": "^0.8.13"
    },
    "devDependencies": {
        "@types/core-js": "0.9.36",
        "@types/jasmine": "^2.5.53",
        "@types/node": "^6.0.45",
        "angular-router-loader": "^0.6.0",
        "angular2-template-loader": "^0.6.2",
        "awesome-typescript-loader": "^3.2.1",
        "del-cli": "^1.1.0",
        "html-loader": "^0.4.5",
        "html-webpack-plugin": "^2.29.0",
        "jasmine-core": "^2.6.4",
        "karma": "^1.7.0",
        "karma-chrome-launcher": "^2.2.0",
        "karma-jasmine": "^1.1.0",
        "karma-jasmine-html-reporter": "^0.2.2",
        "karma-phantomjs-launcher": "^1.0.4",
        "karma-sourcemap-loader": "^0.3.7",
        "karma-webpack": "^2.0.4",
        "lite-server": "^2.3.0",
        "phantomjs-prebuilt": "^2.1.14",
        "raw-loader": "^0.5.1",
        "ts-loader": "^2.3.0",
        "tslint": "^5.5.0",
        "tslint-loader": "^3.5.3",
        "typescript": "^2.4.1",
        "typings": "^2.1.1",
        "webpack": "^2.2.1",
        "webpack-dev-server": "^2.5.1",
        "webpack-merge": "^4.1.0"
    }
}
